<Window x:Name="State" x:Class="Scripts.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Scripts"
        mc:Ignorable="d"
        Title="MainWindow" Height="700" Width="600" WindowStyle="None" Background="Transparent" AllowsTransparency="True" MouseDown="Window_MouseDown" StateChanged="State_StateChanged" Closing="Window_Closing">
    <Grid>
        <!--Использутся для смены страниц-->
        <Frame x:Name="myFrame" NavigationUIVisibility="Hidden" Visibility="Visible"/>
    </Grid>
</Window>
